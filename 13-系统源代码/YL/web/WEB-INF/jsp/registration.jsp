                                <form:select path="adId" class="form-control select2" style="width: 100%;">
                                    <c:forEach items="${administratives}" var="ad" >
                                    <option value="${ad.adId}">${ad.adName}</option>
                                    </c:forEach>
                                </form:select>
                            </div>
                         </div>
                        <!-- /.col -->
                    </div>
                    <!-- /.row -->
                </div>
                <!-- /.box-body -->

            </div>
            <!-- /.box -->

